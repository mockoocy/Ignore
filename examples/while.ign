<program>
    <var name=i type=int>
        {0}
    </var>

    <var name=j type=int>
        {10}
    </var>

    <while condition={i < 5} >
        {print(i)}
        i = {i + 1}
        <while condition={j > 0} >
            {print(j)}
            j = {j - 1}
        </while>
    </while>
  
</program>