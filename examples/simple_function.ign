<program> 
    <var name=a type=int> {3} </var>
    <function name=fibonacci  b:int returnType=int>
        <if condition={a < 4}> 
            <return> 
                {5}
            </return>
        </if>
        <return>
            {3*a}
        </return>
    </function>


    {print(a)}
    {print(fibonacci(3))}
    {print(fibonacci(3))}
    {print(fibonacci(3))} 

</program>