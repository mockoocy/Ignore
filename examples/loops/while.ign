<program>
    <var name=i type=int>
        {0}
    </var>

    <var name=j type=int>
        {10}
    </var>

    <while condition={i < 5} >
         <var name=k type=int>
            {10}
        </var>
        {print("k is", k)}
        i = {i + 1}
        {print("i is", i)}

        <while condition={j > 0} >
            //{print(j)}
            j = {j - 1}
            {print("j is", j)}
            k = {k - 1}
        </while>
    </while>
  
</program>