<program>
    <var name=n type=int> {0} </var>
    <function name=factorial n:int returnType=int>
        <if condition={ n == 0 } >
            <return>
                {1}
            </return>
        </if>
        <return> {65} </return>
    </function> 
    {print(factorial())}
    {print(factorial(15))}
</program>