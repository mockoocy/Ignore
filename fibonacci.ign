<function name=fibonacci 🍌:int returnType=int >
    <if condition={ 🍌 < 0 }>
        <throw exceptionType=InvalidArgumentExeption >
                {"😿😿😿There are no fib number below 0"}
        </throw>
    </if>
    <if condition={🍌 == 1 || 🍌 == 0}> 
        <return>
            {🍌}
        </return>
    </if>
    <return>
        {fibonacci(🍌 - 1) + fibonacci(🍌 - 2)}
    </return>
</function>
